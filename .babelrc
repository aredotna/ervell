{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 versions"]
      }
    }],
    "react",
    "stage-3"
  ],
  "plugins": [
    "transform-runtime",
    ["module-resolver", {
      "extensions": [".js", ".jsx"],
      "root": ["./"]
    }],
    "transform-class-properties"
  ]
}
